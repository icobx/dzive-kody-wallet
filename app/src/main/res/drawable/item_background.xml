<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item >
        <shape android:shape="rectangle" >
            <corners android:radius="10dip" />
            <solid
                android:color="@color/white"
                />
<!--            4DD3D3D3-->
            <padding
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"
                android:left="10dp"
                />
            <stroke
                android:width="1px"
                android:color="#33000000"/>
        </shape>
    </item>
</selector>